<%-include('partials/header');-%>
<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="postTitle">Title</label>
    <input class="form-control" type="text" id="postTitle" name="postTitle" value="" placeholder="Title">
  </div>
  <div class="form-group">
    <label for="postBody">Post</label>
    <textarea class="form-control" name="postBody" id="postBody" rows="8" cols="80"></textarea>
  </div>
  <button class="btn btn-primary btn-md" type="submit" name="publish">Publish</button>
</form>


<%-include('partials/footer');-%>
