<%- include('partials/header'); -%>

<h1>Home</h1>

<p>
  <%= homeStartingContent %>
</p>

<% posts.forEach(function(post){ %>

<h3><%= post.title %></h3>
<p>
  <%= post.content.slice(0,100)+"..."%>
  <a href='/posts/<%= post.title.toLowerCase().replace(" ","-") %>'>Read More</a>
</p>
<% }) %>


<%- include('partials/footer'); -%>
